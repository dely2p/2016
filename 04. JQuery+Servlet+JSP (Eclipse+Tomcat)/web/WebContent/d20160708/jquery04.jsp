<meta charset="UTF-8">
<title>Insert title here</title>
<style type="text/css">
	#content{
		width: 850px;
		background-color: skyblue;
	}
	#quickmenu{
		width: 170px;
		height: 150px;
		border: 3px solid red;
		background-color: orange;
		position: absolute;
		right: 100px;	
		top: 50px;
	}
	#quickmenu ul li{
		list-style-type: none;
	}
</style>
<script type="text/javascript" src="http://ajax.aspnetcdn.com/ajax/jQuery/jquery-3.0.0.min.js"></script>
<script type="text/javascript">
	$(function(){
		//웹페이지가 스크롤될 때 quickmenu가 따라서 이동되게 설정
		
		// 현재 quickmenu의 top의 사이즈 알아오기
		var top = parseInt($("#quickmenu").css("top"));
		// 스크롤 되어질 때 퀵메뉴의 속성값을 스크롤 되어진 top의 위치로 지정
		
		$(window).scroll(function(){
			var scrollTop = $(window).scrollTop();
			var moveTop = scrollTop+top;
			
			$("#quickmenu").stop().animate({top:moveTop},100);
		})
	})
</script>
